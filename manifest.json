{
  "manifest_version": 2,
  "name": "How long are you",
  "version": "1.0",
  "description": "你有多长",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon16.png",
    "48": "images/icon16.png",
    "128": "images/icon16.png"
  },
  "background": {
    "scripts": ["background.js","js/jquery-3.4.1.min.js"],
    "persistent": false
  },
  "permissions": [    
    "http://*/*","storage","alarms","tabs"],
  "page_action": {
    "default_icon": "images/icon16.png",
    "default_popup": "popup.html"
  },
  "commands": {
    "toggle-tags": {
      "suggested_key": {
        "default": "Alt+S",
        "mac": "MacCtrl+S"
      },
      "description": "你有多长"
    }
  },
  "content_scripts":[{
    "matches": [
      "http://*.wondershare.cn/*"
    ],
    "js": [ "content.js","js/jquery-3.4.1.min.js"]
}]
}